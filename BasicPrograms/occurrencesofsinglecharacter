public class CharacterCount {
    public static void main(String[] args) {
        String str = "yogesh Gujrati";
        char searchChar = 'a';
        int count = 0;

        for (int i = 0; i < str.length(); i++) {
            if (str.charAt(i) == searchChar) {
                count++;
            }
        }

        System.out.println("The count of character 'a' is: " + count);
    }
}
